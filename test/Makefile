DEFINES += -DMOXPP_VERSION=\"0.0.0+dev.20250414\" -DDEBUG -DMOXPP_DEBUG -DMOXPP_OS_LINUX -DMOXPP_ARCH_X86_64 -DMOXPP_ARCH_TYPE_X86 -DMOXPP_BIT_64 -DSPDLOG_FMT_EXTERNAL -DSPDLOG_COMPILED_LIB -DMOXPP_APP -DMOXPP_CONSOLE
